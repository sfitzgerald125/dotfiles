// Place your key bindings in this file to override the defaults
[
  // #region Tab Navigation
  {
    "key": "alt+a tab",
    "command": "workbench.action.nextEditor",
    "when":  "!terminalFocus"
  },
  {
    "key": "alt+pagedown",
    "command": "-workbench.action.nextEditor"
  },
  {
    "key": "alt+a shift+tab",
    "command": "workbench.action.previousEditor",
    "when":  "!terminalFocus"
  },
  {
    "key": "alt+pageup",
    "command": "-workbench.action.previousEditor"
  },
  // #endregion
  // #region Navigate panes
  {
    "key": "alt+k",
    "command": "workbench.action.navigateUp"
  },
  {
    "key": "alt+j",
    "command": "workbench.action.navigateDown"
  },
  {
    "key": "alt+h",
    "command": "workbench.action.navigateLeft",
    "when": "vim.mode != 'Insert' && !panelFocus"
  },
  {
    "key": "alt+l",
    "command": "workbench.action.navigateRight"
  },
  {
    "key": "alt+k",
    "command": "workbench.files.action.focusOpenEditorsView",
    "when": "sideBarFocus && explorerViewletVisible"
  },
  {
    "key": "alt+j",
    "command": "workbench.files.action.focusFilesExplorer",
    "when": "sideBarFocus && explorerViewletVisible && !filesExplorerFocus"
  },
  {
    "key": "alt+h",
    "command": "workbench.action.navigateLeft",
    "when": "panelFocus && vim.mode != 'Insert'"
  },
  // #endregion
  // #region Split Editors
  {
    "key": "alt+a shift+\\",
    "command": "workbench.action.splitEditor",
    "when": "!terminalFocus"
  },
  {
    "key": "alt+\\",
    "command": "-workbench.action.splitEditor"
  },
  {
    "key": "alt+a -",
    "command": "workbench.action.splitEditorDown",
    "when":  "!terminalFocus"
  },
  {
    "key": "alt+k alt+\\",
    "command": "-workbench.action.splitEditorDown"
  },
  // #endregion
  // #region Close Editors
  {
    "key": "alt+a x",
    "command": "workbench.action.closeActiveEditor",
    "when":  "!terminalFocus"
  },
  {
    "key": "ctrl+w",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "key": "alt+w",
    "command": "-workbench.action.closeActiveEditor"
  },
  // #endregion
  // #region Workbench toggles
  {
    "key": "shift+ctrl+e",
    "command": "workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "alt+shift+e",
    "command": "-workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "shift+ctrl+f",
    "command": "workbench.action.findInFiles"
  },
  {
    "key": "alt+shift+f",
    "command": "-workbench.action.findInFiles"
  },
  {
    "key": "shift+ctrl+g",
    "command": "workbench.view.scm",
    "when": "workbench.scm.active"
  },
  {
    "key": "alt+shift+g",
    "command": "-workbench.view.scm",
    "when": "workbench.scm.active"
  },
  // #endregion
  // #region File Explorer Shortcuts
  {
    "key": "enter",
    "command": "-renameFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "r",
    "command": "renameFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "a",
    "command": "explorer.newFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "p",
    "command": "filesExplorer.paste",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "y",
    "command": "filesExplorer.copy",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !inputFocus"
  },
  {
    "key": "d",
    "command": "deleteFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !inputFocus"
  },
  {
    "key": "x",
    "command": "filesExplorer.cut",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "alt+u",
    "command": "list.focusPageUp",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "alt+d",
    "command": "list.focusPageDown",
    "when": "listFocus && !inputFocus"
  },
  // #endregion
  // #region Folding
  {
    "key": "alt+a f",
    "command": "editor.fold",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "alt+a ctrl+f",
    "command": "editor.foldAll",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "alt+a ctrl+1",
    "command": "editor.foldLevel1",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "alt+a ctrl+2",
    "command": "editor.foldLevel2",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "alt+a ctrl+3",
    "command": "editor.foldLevel3",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "alt+a ctrl+4",
    "command": "editor.foldLevel4",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "alt+a ctrl+5",
    "command": "editor.foldLevel5",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "alt+a ctrl+6",
    "command": "editor.foldLevel6",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "alt+a ctrl+7",
    "command": "editor.foldLevel7",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  // #endregion
  // #region Unfolding
  {
    "key": "alt+a u",
    "command": "editor.unfold",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "alt+a ctrl+u",
    "command": "editor.unfoldAll",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "ctrl+f",
    "command": "editor.action.startFindReplaceAction",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "alt+ctrl+f",
    "command": "-editor.action.startFindReplaceAction",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "alt+ctrl+f",
    "command": "actions.find",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "ctrl+f",
    "command": "-actions.find",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "alt+d",
    "command": "-deleteRight",
    "when": "textInputFocus"
  },
  {
    "key": "alt+d",
    "command": "-list.focusPageDown",
    "when": "listFocus && !inputFocus"
  },
  // #endregion
  // #region Terminal Navigation
  {
    "key": "alt+a m",
    "command": "workbench.action.toggleMaximizedPanel",
    "when": "panelFocus"
  },
  {
    "key": "alt+a n",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "alt+a p",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "alt+pagedown",
    "command": "-workbench.action.terminal.focusNext",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "alt+pageup",
    "command": "-workbench.action.terminal.focusPrevious",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "alt+a ,",
    "command": "workbench.action.terminal.rename"
  },
  {
    "key": "alt+a c",
    "command": "workbench.action.terminal.new",
    "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
  },
  {
    "key": "alt+shift+`",
    "command": "-workbench.action.terminal.new",
    "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
  },
  {
    "key": "alt+a x",
    "command": "workbench.action.terminal.kill",
  },
  // #endregion
  // #region Quick Open
  {
    "key": "alt+n",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  {
    "key": "alt+p",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "shift+ctrl+p",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "f1",
    "command": "-workbench.action.showCommands"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "alt+p",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "alt+e",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "alt+n",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
  },
  {
    "key": "down",
    "command": "-selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
  },
  {
    "key": "alt+p",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
  },
  {
    "key": "up",
    "command": "-selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
  },
  // #endregion
  // #region vim keymaps
  {
    "key": "alt+h",
    "command": "editor.action.triggerSuggest",
    "when": "vim.mode == 'Insert' && editorHasCompletionItemProvider && textInputFocus && !editorReadonly && !suggestWidgetVisible"
  },
  {
    "key": "ctrl+i",
    "command": "-editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly && !suggestWidgetVisible"
  },
  {
    "key": "alt+r",
    "command": "extension.vim_ctrl+r",
    "when": "editorTextFocus && vim.active && vim.use<C-r> && !inDebugRepl"
  },
  {
    "key": "alt+d",
    "command": "extension.vim_ctrl+d",
    "when": "editorTextFocus && vim.active && vim.use<C-d> && !inDebugRepl"
  },
  {
    "key": "ctrl+d",
    "command": "-extension.vim_ctrl+d",
    "when": "editorTextFocus && vim.active && vim.use<C-d> && !inDebugRepl"
  },
  {
    "key": "alt+u",
    "command": "extension.vim_ctrl+u",
    "when": "editorTextFocus && vim.active && vim.use<C-u> && !inDebugRepl"
  },
  {
    "key": "ctrl+u",
    "command": "-extension.vim_ctrl+u",
    "when": "editorTextFocus && vim.active && vim.use<C-u> && !inDebugRepl"
  },
  // #endregion
  // #region alt/ctrl remaps
  {
    "key": "ctrl+n",
    "command": "workbench.action.files.newUntitledFile"
  },
  {
    "key": "alt+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+`",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "h",
    "command": "previousCompressedFolder",
    "when": "explorerViewletCompressedFocus && filesExplorerFocus && foldersViewVisible && !explorerViewletCompressedFirstFocus && !inputFocus"
  },
  {
    "key": "left",
    "command": "-previousCompressedFolder",
    "when": "explorerViewletCompressedFocus && filesExplorerFocus && foldersViewVisible && !explorerViewletCompressedFirstFocus && !inputFocus"
  },
  {
    "key": "l",
    "command": "nextCompressedFolder",
    "when": "explorerViewletCompressedFocus && filesExplorerFocus && foldersViewVisible && !explorerViewletCompressedLastFocus && !inputFocus"
  },
  {
    "key": "right",
    "command": "-nextCompressedFolder",
    "when": "explorerViewletCompressedFocus && filesExplorerFocus && foldersViewVisible && !explorerViewletCompressedLastFocus && !inputFocus"
  },
  // #endregion
]