// Place your key bindings in this file to override the defaults
[
  // #region Tab Navigation
  {
    "key": "ctrl+a tab",
    "command": "workbench.action.nextEditor",
    "when":  "!terminalFocus"
  },
  {
    "key": "ctrl+pagedown",
    "command": "-workbench.action.nextEditor"
  },
  {
    "key": "ctrl+a shift+tab",
    "command": "workbench.action.previousEditor",
    "when":  "!terminalFocus"
  },
  {
    "key": "ctrl+pageup",
    "command": "-workbench.action.previousEditor"
  },
  // #endregion
  // #region Navigate panes
  {
    "key": "ctrl+k",
    "command": "workbench.action.navigateUp"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.navigateDown"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.navigateLeft",
    "when": "vim.mode != 'Insert' && !panelFocus"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.navigateRight"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.files.action.focusOpenEditorsView",
    "when": "sideBarFocus && explorerViewletVisible"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.files.action.focusFilesExplorer",
    "when": "sideBarFocus && explorerViewletVisible && !filesExplorerFocus"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.navigateLeft",
    "when": "panelFocus && vim.mode != 'Insert'"
  },
  // #endregion
  // #region Split Editors
  {
    "key": "ctrl+a shift+\\",
    "command": "workbench.action.splitEditor",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+\\",
    "command": "-workbench.action.splitEditor"
  },
  {
    "key": "ctrl+a -",
    "command": "workbench.action.splitEditorDown",
    "when":  "!terminalFocus"
  },
  {
    "key": "ctrl+k ctrl+\\",
    "command": "-workbench.action.splitEditorDown"
  },
  // #endregion
  // #region Close Editors
  {
    "key": "ctrl+a x",
    "command": "workbench.action.closeActiveEditor",
    "when":  "!terminalFocus"
  },
  {
    "key": "meta+w",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "key": "ctrl+w",
    "command": "-workbench.action.closeActiveEditor"
  },
  // #endregion
  // #region Workbench toggles
  {
    "key": "shift+meta+e",
    "command": "workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "ctrl+shift+e",
    "command": "-workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "shift+meta+f",
    "command": "workbench.action.findInFiles"
  },
  {
    "key": "ctrl+shift+f",
    "command": "-workbench.action.findInFiles"
  },
  {
    "key": "shift+meta+g",
    "command": "workbench.view.scm",
    "when": "workbench.scm.active"
  },
  {
    "key": "ctrl+shift+g",
    "command": "-workbench.view.scm",
    "when": "workbench.scm.active"
  },
  // #endregion
  // #region File Explorer Shortcuts
  {
    "key": "enter",
    "command": "-renameFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "r",
    "command": "renameFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "a",
    "command": "explorer.newFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "p",
    "command": "filesExplorer.paste",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "y",
    "command": "filesExplorer.copy",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !inputFocus"
  },
  {
    "key": "d",
    "command": "deleteFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !inputFocus"
  },
  {
    "key": "x",
    "command": "filesExplorer.cut",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "ctrl+u",
    "command": "list.focusPageUp",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "ctrl+d",
    "command": "list.focusPageDown",
    "when": "listFocus && !inputFocus"
  },
  // #endregion
  // #region Folding
  {
    "key": "ctrl+a f",
    "command": "editor.fold",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "ctrl+a meta+f",
    "command": "editor.foldAll",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "ctrl+a meta+1",
    "command": "editor.foldLevel1",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "ctrl+a meta+2",
    "command": "editor.foldLevel2",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "ctrl+a meta+3",
    "command": "editor.foldLevel3",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "ctrl+a meta+4",
    "command": "editor.foldLevel4",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "ctrl+a meta+5",
    "command": "editor.foldLevel5",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "ctrl+a meta+6",
    "command": "editor.foldLevel6",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "ctrl+a meta+7",
    "command": "editor.foldLevel7",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  // #endregion
  // #region Unfolding
  {
    "key": "ctrl+a u",
    "command": "editor.unfold",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "ctrl+a meta+u",
    "command": "editor.unfoldAll",
    "when": "editorTextFocus && foldingEnabled && !terminalFocus"
  },
  {
    "key": "meta+f",
    "command": "editor.action.startFindReplaceAction",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "alt+meta+f",
    "command": "-editor.action.startFindReplaceAction",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "alt+meta+f",
    "command": "actions.find",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "meta+f",
    "command": "-actions.find",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "ctrl+d",
    "command": "-deleteRight",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+d",
    "command": "-list.focusPageDown",
    "when": "listFocus && !inputFocus"
  },
  // #endregion
  // #region Terminal Navigation
  {
    "key": "ctrl+a m",
    "command": "workbench.action.toggleMaximizedPanel",
    "when": "panelFocus"
  },
  {
    "key": "ctrl+a n",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "ctrl+a p",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "ctrl+pagedown",
    "command": "-workbench.action.terminal.focusNext",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "ctrl+pageup",
    "command": "-workbench.action.terminal.focusPrevious",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "ctrl+a ,",
    "command": "workbench.action.terminal.rename"
  },
  {
    "key": "ctrl+a c",
    "command": "workbench.action.terminal.new",
    "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
  },
  {
    "key": "ctrl+shift+`",
    "command": "-workbench.action.terminal.new",
    "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
  },
  {
    "key": "ctrl+a x",
    "command": "workbench.action.terminal.kill",
  },
  // #endregion
  // #region Quick Open
  {
    "key": "ctrl+n",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "shift+meta+p",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "f1",
    "command": "-workbench.action.showCommands"
  },
  {
    "key": "meta+p",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+p",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "ctrl+e",
    "command": "-workbench.action.quickOpen"
  },
  // #endregion
  // #region alt/ctrl remaps
  {
    "key": "meta+n",
    "command": "workbench.action.files.newUntitledFile"
  },
  {
    "key": "ctrl+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  // #endregion
]