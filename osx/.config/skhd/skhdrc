# stack vs bsp
alt - e : yabai -m space --layout bsp
alt - s : yabai -m space --layout stack

# navigate windows stack or bsp
alt - j : yabai -m window --focus stack.prev || yabai -m window --focus south
alt - k : yabai -m window --focus stack.next || yabai -m window --focus north
alt - h : yabai -m window --focus stack.prev || yabai -m window --focus west
alt - l : yabai -m window --focus stack.next || yabai -m window --focus east

# Swap windows
shift + alt - j : yabai -m window --swap south
shift + alt - k : yabai -m window --swap north
shift + alt - h : yabai -m window --swap west
shift + alt - l : yabai -m window --swap east

# Move window to space #
shift + alt - 1 : yabai -m window --space 1
shift + alt - 2 : yabai -m window --space 2
shift + alt - 3 : yabai -m window --space 3
shift + alt - 4 : yabai -m window --space 4
shift + alt - 5 : yabai -m window --space 5
shift + alt - 6 : yabai -m window --space 6
shift + alt - 7 : yabai -m window --space 7

# Toggle float
shift + alt - space : yabai -m window --toggle float --grid 4:4:1:1:2:2

# Toggle Split
alt - t : yabai -m window --toggle split
